<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:UiManager="http://fundp.ac.be/UiManager/" xmlns:partnerWS="http://www.example.org/partnerWS/" xmlns:travelcompany.org.be="http://travelcompany.org.be">
  <process name="travelcompany.org.be:PurchaseOrder">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="travelcompany.org.be:PurchaseOrder" port="PurchaseOrderPort"/>
    </provide>
    <invoke partnerLink="MyPartnerLN">
      <service name="partnerWS:partnerWS" port="partnerWSSOAP"/>
    </invoke>
    <invoke partnerLink="name">
      <service name="UiManager:UiManager" port="UiManagerSOAP"/>
    </invoke>
  </process>
</deploy>